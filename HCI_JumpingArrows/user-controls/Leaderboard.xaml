<UserControl x:Class="HCI_JumpingArrows.user_controls.Leaderboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HCI_JumpingArrows.user_controls"
             mc:Ignorable="d" 
             d:DesignHeight="480" d:DesignWidth="368">
    <Grid>
        <Label Content="Jumping Arrows" FontSize="30" FontWeight="DemiBold" HorizontalAlignment="Center" Height="55" Margin="0,60,0,0" VerticalAlignment="Top" Width="auto"/>
        <Label Content="Press SPACE to start new game!" FontWeight="DemiBold" FontSize="18" HorizontalAlignment="Center" Height="35" Margin="0,300,0,0" VerticalAlignment="Top" Width="auto"/>
        <TextBlock x:Name="Leader" TextAlignment="Left" HorizontalAlignment="Center" Height="180" Margin="0,120,0,0" Foreground="#FF3F3D3D" VerticalAlignment="Top" Width="auto" FontSize="14" FontWeight="Medium"/>
    </Grid>
</UserControl>
